{
    "name": "howtocodewell/doctrine-entity-traits",
    "type": "package",
    "description": "Entity traits for How To Code Well",
    "version": "1.0.0",
    "minimum-stability": "dev",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "HowToCodeWell\\Entity\\": "src/Entity"
        }
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpstan/phpstan": "^0.12.14",
        "codeception/codeception": "^4.1",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0"
    },
    "require": {
        "php": ">=7.2",
        "ramsey/uuid": "^3.9",
        "doctrine/orm": "^2.7"
    }
}